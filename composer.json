{
    "name": "lorenzosapora/email-builder",
    "description": "Flexible Content & Repeater Fields for Laravel Nova.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "flexible",
        "repeat",
        "group",
        "layout"
    ],
    "authors": [
        {
            "name": "Lorenzo Sapora",
            "email": "lorenzo@sapora.co"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "LorenzoSapora\\EmailBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LorenzoSapora\\EmailBuilder\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
